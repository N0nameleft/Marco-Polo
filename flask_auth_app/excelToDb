import sqlite3
import pandas as pd

path = "/Users/zach/Desktop/Aus/UWA/CITS3403/Project/Project 2/Akinator-clone/flask_auth_app/backupdb.xls"
dx = pd.read_excel(path, sheet_name="countryCode")
db = sqlite3.connect('sqlite.db')

for table, df in dx.items():
    df.to_sql(table, db)
    print(f'{df} inserted successfully')